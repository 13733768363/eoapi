{
  "name": "eoapi",
  "version": "0.0.1",
  "description": "API ToolKit",
  "homepage": "https://github.com/eolinker/eoapi.git",
  "author": {
    "name": "eoapi",
    "email": "dev@eolink.com"
  },
  "keywords": [
    "angular",
    "angular 13",
    "electron",
    "electron 16",
    "nodejs",
    "typescript",
    "playwright",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "postinstall": "(cd app && npm install)&&electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "build:web": "npm run electron:serve-tsc && ng build -c production",
    "ng:serve": "ng serve -c web -o",
    "ng:web": "ng serve -c webaot -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:dev":"npm run electron:serve-tsc && electron . --development",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:build": "npm run build:prod && electron-builder build",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:web": "npx playwright test e2e/cases/",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "release": "electron-builder"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "conventional-changelog-cli": "2.2.2",
    "electron": "17.1.2",
    "electron-builder": "^22.14.13",
    "electron-reload": "1.5.0",
    "eslint": "8.10.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "37.9.7",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "4.0.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.17",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.1.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "lodash": "4.17.21",
    "node-polyfill-webpack-plugin": "1.1.4",
    "npm-run-all": "4.1.5",
    "ts-node": "10.7.0",
    "typescript": "~4.6.2",
    "wait-on": "6.0.1"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "browserslist": [
    "chrome 98"
  ],
  "__npminstall_done": false
}
