{
  "name": "eoapi-core",
  "version": "0.0.1",
  "main": "app/electron-main/main.js",
  "private": true,
  "scripts": {
    "postinstall": "npm-run-all --serial install:*",
    "start": "npm-run-all  -p startall electron:serve",
    "startall": "npm-run-all -p start:*",
    "install-stash:api-manager": "cd core/api-manager/browser&&npm ci",
    "start:api-manager": "cd core/api-manager/browser&&npm start",
    "install:workbench": "cd workbench/browser&&npm insall",
    "start:workbench": "cd workbench/browser&&npm start",
    "install:node-electron": "electron-builder install-app-deps",
    "electron:serve": "wait-on tcp:4200 && npm run electron:dev",
    "electron:dev:static": "npm run electron:tsc && electron .",
    "electron:dev": "npm run electron:tsc && electron . --development",
    "release": "electron-builder",
    "electron:tsc": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "electron-log": "^4.4.6",
    "electron-updater": "^4.6.5",
    "resolve": "^1.22.0"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "electron": "17.1.2",
    "electron-builder": "^22.14.13",
    "electron-reload": "1.5.0",
    "npm-run-all": "4.1.5",
    "ts-node": "10.7.0",
    "typescript": "~4.6.2",
    "eslint": "8.10.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "37.9.7",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "wait-on": "6.0.1"
  },
  "__npminstall_done": false
}
